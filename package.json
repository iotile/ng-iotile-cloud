{
  "name": "ng-iotile-cloud",
  "version": "0.11.4",
  "description": "Angular IOTile Cloud interface",
  "main": "ng-iotile-cloud.js",
  "scripts": {
    "start": "static",
    "peer": "npm install es6-promise@^3.0.2 es6-shim@^0.33.3 reflect-metadata@0.1.2 rxjs@5.0.3 zone.js@0.6.26",
    "lint": "tslint ./src/**/*.ts",
    "clean": "rimraf bundles iotile **/*.ngfactory.ts **/*.ngsummary.json",
    "tsc": "ngc -p .",
    "tsc:single": "tsc -p tsconfig.bundle.json",
    "uglify": "uglifyjs ./bundles/ng-iotile-cloud.js --output ./bundles/ng-iotile-cloud.min.js",
    "bundle": "npm run tsc:single && npm run uglify",
    "build": "npm run clean && npm run typings && npm run lint && npm run tsc && npm run bundle",
    "prepublish": "npm run build",
    "typings": "typings install",
    "test": "karma start",
    "test:w": "karma start --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iotile/ng2-iotile-cloud.git"
  },
  "keywords": [
    "angular2",
    "iotile",
    "iot"
  ],
  "author": "Arch Systems<help@arch-iot.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iotile/ng-iotile-cloud/issues"
  },
  "homepage": "https://github.com/iotile/ng-iotile-cloud#readme",
  "devDependencies": {
    "@angular/common": ">=2.4.1 < 4.0.0",
    "@angular/compiler": ">=2.4.1 < 4.0.0",
    "@angular/compiler-cli": ">=2.4.1 < 4.0.0",
    "@angular/core": ">=2.4.1 < 4.0.0",
    "@angular/http": ">=2.4.1 < 4.0.0",
    "@angular/platform-browser": ">=2.4.1 < 4.0.0",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.8.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-remap-istanbul": "^0.2.1",
    "karma-spec-reporter": "0.0.24",
    "moment": "^2.17.1",
    "node-static": "^0.7.8",
    "protractor": "4.0.5",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.5.1",
    "rxjs": "^5.0.3",
    "systemjs": "^0.19.38",
    "ts-helpers": "^1.1.1",
    "ts-node": "1.2.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "typings": "^1.3.3",
    "uglify-js": "^2.6.2",
    "zone.js": "^0.7.2"
  },
  "peerDependencies": {
    "@angular/common": ">=2.4.1 < 5.0.0",
    "@angular/compiler": ">=2.4.1 < 5.0.0",
    "@angular/core": ">=2.4.1 < 5.0.0",
    "@angular/http": ">=2.4.1 < 5.0.0",
    "@angular/platform-browser": ">=2.4.1 < 5.0.0"
  },
  "dependencies": {}
}
