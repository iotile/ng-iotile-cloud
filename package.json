{
  "name": "ng-iotile-cloud",
  "version": "0.11.9",
  "description": "Angular IOTile Cloud interface",
  "keywords": [
    "angular 2",
    "iotile",
    "iot"
  ],
  "main": "ng-iotile-cloud.js",
  "scripts": {
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "./node_modules/uglify-js/bin/uglifyjs dist/bundles/ng-iotile-cloud.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng-iotile-cloud.umd.min.js",
    "copy": "cpx './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/*",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy",
    "test": "karma start"
  },
  "license": "MIT",
  "homepage": "https://github.com/iotile/ng-iotile-cloud",
  "bugs": {
    "url": "https://github.com/iotile/ng-iotile-cloud/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iotile/ng-iotile-cloud.git"
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.4",
    "@angular/compiler-cli": "^2.4.4",
    "cpx": "^1.5.0",
    "renamer": "^0.6.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "typescript": "~2.0.0",
    "uglify-js": "^2.7.5",
    "jasmine": "2.8.0",
    "jasmine-core": "2.8.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-remap-istanbul": "0.2.1",
    "karma-spec-reporter": "0.0.24",
    "moment": "^2.17.1"
  },
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/http": "^2.4.4",
    "@angular/core": "^2.4.4",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.6"
  }
}
